<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="ct" class="com.autowireconstructor.Categories" autowire="constructor">
    <property name="name" value="Programming Books"/>
</bean>

<bean id="bk" class="com.autowireconstructor.Book">
    <property name="bookName" value="Complete Java Reference"/>
    <property name="bookPrice" value="99"/>
</bean>

</beans>