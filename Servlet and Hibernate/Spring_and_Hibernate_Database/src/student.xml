<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sph.Student" table="stud">
        <id name="id" column="id"/>
        <property name="name" column="name" length="15"/>
        <property name="marks" column="marks" length="10"/>

    </class>
</hibernate-mapping>